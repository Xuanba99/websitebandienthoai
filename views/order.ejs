      <h1 class="dondathang">Đơn đặt hàng</h1>
      <div class="dondathang-content">
        <table border="1" class="tableproduct">
          <tr>
            <th width="50">ID</th>
            <th width="120">Tên khách hàng</th>
            <th width="150">Địa chỉ</th>
            <th width="150">Ngày đặt</th>
            <th width="80">Số điệnt thoại</th>
            <th width="150">Sản phẩm</th>
            <th width="50">số lượng</th>
            <th width="70">Tổng</th>
            <th width="80">Trạng thái</th>
            <th width="110">Nâng cao</th>
          </tr>
          <% orders.map(item=>{%>
          <tr>
            <td><a href="/admin/order?id=<%= item._id%>"><%= item._id%></a></td>
            <td><%= item.cusname%></td>
            <td><%= item.cusaddress%></td>
            <td><%= new Date(item.dateorder).toLocaleString()%></td>
            <td><%= item.cusphone%></td>
            <td><%= item.nameofproduct%></td>
            <td><%= item.quantity%></td>
            <td><%= item.total%></td>
            <td><%= item.status%></td>
            <td><a href="/admin/updatestatus?id=<%= item._id%>&status=APPROVE">Approve</a> || <a
                href="/admin/updatestatus?id=<%= item._id%>&status=CANCEL">Cancel</a></td>
          </tr>
          <%})%>
        </table>
